var exam1, exam2, final, project1, project2, project3, project4, labs, grade;
begin
    read exam1;
    read exam2;
    read final;
    read project1;
    read project2;
    read project3;
    read project4;
    read labs;
    grade := (exam1 / 5) + (exam2 / 5) + (final / 4) + ((project1 + project2 + project3 + project4) / 13) + (labs / 20);
    write grade;
end.
